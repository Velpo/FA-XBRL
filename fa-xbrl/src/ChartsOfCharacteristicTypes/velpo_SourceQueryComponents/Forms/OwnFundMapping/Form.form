    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	SourceQueryComponents.Ref,
	SourceQueryComponents.Code,
	SourceQueryComponents.Description,
	SourceQueryComponents.ValueType,
	SourceQueryComponents.ComponentType,
	CASE
		WHEN ItemLinks.Owner IS NULL 
			THEN FALSE
		ELSE TRUE
	END AS HasLink
FROM
	ChartOfCharacteristicTypes.velpo_SourceQueryComponents AS SourceQueryComponents
	
	LEFT JOIN 
