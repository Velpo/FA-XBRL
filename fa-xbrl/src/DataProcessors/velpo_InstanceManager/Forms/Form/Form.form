    <name>RoleTables</name>
    <title>
      <key>ru</key>
      <value>Таблицы</value>
    </title>
    <title>
      <key>en</key>
      <value>Tables</value>
    </title>
    <id>2</id>
    <valueType>
      <types>DynamicList</types>
    </valueType>
    <view>
      <common>true</common>
    </view>
    <edit>
      <common>true</common>
    </edit>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>RoleTables.Changed</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>RoleTables.Instance</segments>
    </notDefaultUseAlwaysAttributes>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	Catalog_RoleTables.Ref AS RoleTable,
	Catalog_RoleTables.Description,
	Catalog_RoleTables.RoleType.LongDescription AS LongDescription,
	Catalog_RoleTables.Name,
	CASE
		WHEN Document_Instance.Ref IS NULL THEN
			FALSE
		ELSE
			TRUE
	END AS Changed,
	ISNULL(Document_Instance.Ref, VALUE(Document.velpo_Instance.EmptyRef)) AS Instance,
	Document_Instance.User  AS User,
	Document_Instance.ChangeDate AS ChangeDate,
	Document_Instance.Comment AS Comment
FROM
	Catalog.velpo_RoleTables AS Catalog_RoleTables

INNER JOIN Catalog.velpo_EntryPoints.RoleTables AS Catalog_EntryPoints_RoleTables
ON Catalog_RoleTables.Ref = Catalog_EntryPoints_RoleTables.RoleTable
	AND Catalog_EntryPoints_RoleTables.Ref = &amp;EntryPoint
	
LEFT JOIN Document.velpo_Instance AS Document_Instance
ON Catalog_RoleTables.Ref = Document_Instance.RoleTable
	AND Document_Instance.Date = &amp;Period
	AND Document_Instance.BusinessUnit = &amp;BusinessUnit
	
WHERE
	Catalog_RoleTables.Owner = &amp;Taxonomy</queryText>
